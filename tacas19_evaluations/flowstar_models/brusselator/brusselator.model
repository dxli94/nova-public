continuous reachability
{
 state var x, y
 setting
 {
        fixed steps 0.01
        time 25
        remainder estimation 1e-4
        QR precondition
        gnuplot octagon x,y
        fixed orders 6
        cutoff 1e-9
        precision 53
        output out
        print on
 }

 poly ode 1
 {
  x' = 1 + x*(x*y - 2.5)
  y' = x*(1.5 - x*y)
 }

 init
 {
  x in [0.8,1]
  y in [0,0.2]
 }
}


unsafe
{
  y >= 2
}
